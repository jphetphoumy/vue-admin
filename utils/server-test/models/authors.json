{
  "config": {
    "locale": "en"
  },
  "model": {
    "id": {
      "type": "randomNumberBetween",
      "value": [1, 2500000]
    },
    "name": {
      "type": "faker",
      "value": "name.firstName"
    },
    "lastname" : {
      "type": "faker",
      "value": "name.lastName"
    },
    "birtdate": {
      "type": "faker",
      "value": "date.between",
      "options": ["1970-01-01","1980-01-01"]
    }
  }
}
